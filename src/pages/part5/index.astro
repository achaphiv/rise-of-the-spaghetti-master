---
import NavigationLinks from '../../components/NavigationLinks.astro'
import ScenesDisplay from '../../components/ScenesDisplay.astro'
import StackY from '../../components/StackY.astro'
import {
  NANI,
  aboutToScream,
  heBelieves,
  scream,
  spaghettiWave,
  strandsBehind,
  strandsHangingOff,
  strandsLeftHand,
  swirlingActionShot,
} from '../../images'
import Layout from '../../layouts/Layout.astro'

const navLinks = { back: './part4', forward: './part6' }

const scenes = [
  { src: aboutToScream, description: '' },
  { src: scream, description: '' },
  { src: strandsHangingOff, description: '' },
  { src: heBelieves, description: '' },
  { src: strandsBehind, description: 'Super saiyan????' },
  { src: strandsLeftHand, description: '' },
  { src: swirlingActionShot, description: '' },
  { src: spaghettiWave, description: 'SPAGHETTI WAVE' },
  { src: NANI, description: 'NANI!!!!!!' },
]
---

<Layout titleExtra="Part 5">
  <StackY>
    <NavigationLinks links={navLinks} />

    <ScenesDisplay scenes={scenes} />

    <NavigationLinks links={navLinks} />
  </StackY>
</Layout>
